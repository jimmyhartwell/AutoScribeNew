                                                <SymbolIcon x:Name='PlayPauseSymbol' Symbol="Play"/>
                                            </AppBarButton.Icon>
                                        </AppBarButton>
                                    </CommandBar>
                                </Border>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
